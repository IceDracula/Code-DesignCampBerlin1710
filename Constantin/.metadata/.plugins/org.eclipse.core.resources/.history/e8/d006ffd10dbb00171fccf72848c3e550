package cc.game;

public class move_aktion {
	
	public static int Laptopint;
	public static int Telefonint;
	
	public void Laptop(){
		if(Laptopint == 1){
			//animation
			Laptopint --;
			
		}
		
			
		
	}
	
	public void Telefon(){
		if(Telefonint== 1){
			//animation
			Telefonint -- ;
		}
	}
	
	

}
